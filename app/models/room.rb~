class Room < ActiveRecord::Base
  belongs_to :hotel
   self.inheritance_column = nil
  def self.getrooms(name,from_date,to_date)
@list=Room.find_by_sql(["select r.number, r.type , r.price from rooms as r , hotels as h 
 where  r.hotel_id = h.id and  h.name=? and r.id not in(select  distinct room_id
 from res_rooms as res
where  res.chkin_date   between  ? and ?
and res.chkout_date   between ? and ? )",name,from_date,to_date,from_date,to_date])
end
end
